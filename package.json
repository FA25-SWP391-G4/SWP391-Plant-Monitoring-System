{
  "name": "plant-system",
  "version": "1.0.0",
  "private": true,
  "description": "Plant Monitoring System with IoT integration",
  "main": "app.js",
  "dependencies": {
    "@tensorflow/tfjs": "^4.15.0",
    "@tensorflow/tfjs-backend-cpu": "^4.15.0",
    "aws-iot-device-sdk": "^2.2.12",
    "aws-iot-device-sdk-v2": "^1.23.1",
    "aws-sdk": "^2.1598.0",
    "axios": "^1.12.2",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "body-parser": "^2.2.0",
    "compression": "^1.8.1",
    "concurrently": "^9.2.1",
    "connect-pg-simple": "^10.0.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "csv-writer": "^1.6.0",
    "dateformat": "^5.0.3",
    "debug": "~2.6.9",
    "dotenv": "^17.2.3",
    "ejs": "^3.1.10",
    "explore": "^0.2.0",
    "express": "~4.16.1",
    "express-rate-limit": "^7.2.0",
    "express-session": "^1.18.2",
    "express-validator": "^7.2.1",
    "firebase-admin": "^13.5.0",
    "googleapis": "^164.1.0",
    "http-errors": "~1.6.3",
    "jade": "^1.11.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.18.2",
    "morgan": "~1.9.1",
    "mqtt": "^5.5.0",
    "multer": "^2.0.2",
    "node-cache": "^5.1.2",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.6",
    "openai": "^6.2.0",
    "pg": "^8.16.3",
    "qs": "^6.14.0",
    "react-error-boundary": "^6.0.0",
    "request": "^2.88.2",
    "sequelize": "^6.37.7",
    "sharp": "^0.34.4",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.8.1",
    "uuid": "^13.0.0",
    "vnpay": "^2.4.4",
    "winston": "^3.13.0"
  },
  "scripts": {
    "start": "node ./bin/www",
    "start:dev": "concurrently \"npm start --prefix client\" \"nodemon ./bin/www\"",
    "start:all": "concurrently \"npm run start:dev\" \"npm run start:ai\"",
    "start:ai": "cd ai_service && uvicorn main:app --reload",
    "dev:client": "cd client && npm run dev",
    "dev:ai": "cd ai_service && npm run dev",
    "start:client": "cd client && npm start",
    "start:ai-service": "cd ai_service && npm start",
    "dev:all": "concurrently \"nodemon ./bin/www\" \"npm run dev:client\" \"npm run dev:ai\"",
    "start:all-services": "concurrently \"./bin/www\" \"npm run start:client\" \"npm run start:ai-service\"",
    "install:all": "npm install && npm --prefix client install",
    "build": "npm --prefix client run build",
    "test": "jest --projects jest.config.json",
    "test:unit": "jest --projects tests/unit --testPathPattern=tests/unit",
    "test:integration": "jest --projects tests/integration --testPathPattern=tests/integration",
    "test:ui": "jest --projects tests/ui --testPathPattern=tests/ui",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:ui",
    "test:setup": "node tests/test-setup.js",
    "test:watch": "jest --watch --projects jest.config.json",
    "test:watch:unit": "jest --watch --projects tests/unit --testPathPattern=tests/unit",
    "test:watch:integration": "jest --watch --projects tests/integration --testPathPattern=tests/integration",
    "test:watch:ui": "jest --watch --projects tests/ui --testPathPattern=tests/ui",
    "test:coverage": "jest --coverage --projects jest.config.json",
    "test:coverage:unit": "jest --coverage --projects tests/unit --testPathPattern=tests/unit",
    "test:coverage:integration": "jest --coverage --projects tests/integration --testPathPattern=tests/integration",
    "test:i18n": "node scripts/i18n-integrity-checker.js",
    "test:sensor": "jest tests/sensor.test.js",
    "test:mqtt": "jest tests/mqtt.test.js",
    "test:aws": "jest tests/aws-iot.test.js",
    "test:payment": "node run-payment-tests.js",
    "test:payment:watch": "jest tests/payment*.test.js --watch"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "proxy": "http://localhost:3010",
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.28.5",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/selenium-webdriver": "^4.35.3",
    "babel-jest": "^30.2.0",
    "chromedriver": "^142.0.1",
    "cross-env": "^10.1.0",
    "geckodriver": "^6.0.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-environment-node": "^30.2.0",
    "jest-transform-stub": "^2.0.0",
    "jsdom": "^27.2.0",
    "nodemon": "^3.0.1",
    "selenium-webdriver": "^4.38.0",
    "supertest": "^7.1.4"
  }
}
