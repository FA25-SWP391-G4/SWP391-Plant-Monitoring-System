{
  "name": "plant-system",
  "version": "1.0.0",
  "private": true,
  "description": "Plant Monitoring System with IoT integration",
  "main": "app.js",
  "dependencies": {
    "aws-iot-device-sdk": "^2.2.12",
    "aws-iot-device-sdk-v2": "^1.23.0",
    "aws-sdk": "^2.1598.0",
    "axios": "^1.12.2",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "body-parser": "^2.2.0",
    "concurrently": "^9.2.1",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "dateformat": "^5.0.3",
    "debug": "~2.6.9",
    "dotenv": "^17.2.2",
    "ejs": "^3.1.10",
    "express": "~4.16.1",
    "express-rate-limit": "^7.5.1",
    "express-validator": "^7.2.1",
    "firebase-admin": "^13.5.0",
    "http-errors": "~1.6.3",
    "jade": "^1.11.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.18.2",
    "morgan": "~1.9.1",
    "mqtt": "^5.14.1",
    "multer": "^2.0.2",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.6",
    "openai": "^6.2.0",
    "pg": "^8.16.3",
    "qs": "^6.14.0",
    "socket.io": "^4.7.5",
    "winston": "^3.13.0"
  },
  "scripts": {
    "start": "node ./bin/www",
    "start:dev": "concurrently \"npm start --prefix client\" \"nodemon ./bin/www\"",
    "start:all": "concurrently \"npm run start:dev\" \"npm run start:ai\"",
    "start:ai": "cd ai_service && uvicorn main:app --reload",
    "dev": "node scripts/start-dev.js",
    "dev:quick": "node scripts/quick-test.js",
    "dev:infrastructure": "docker-compose -f docker-compose.dev.yml up -d",
    "dev:stop": "docker-compose -f docker-compose.dev.yml down",
    "install:all": "npm install && npm --prefix client install",
    "build": "npm --prefix client run build",
    "test": "jest",
    "test:setup": "node tests/test-setup.js",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:i18n": "node scripts/i18n-integrity-checker.js",
    "test:sensor": "jest tests/sensor.test.js",
    "test:mqtt": "jest tests/mqtt.test.js",
    "test:aws": "jest tests/aws-iot.test.js",
    "test:e2e": "node tests/run-ai-e2e-tests.js",
    "test:e2e:complete": "node tests/run-complete-e2e-suite.js",
    "test:e2e:ai": "jest tests/ai-features-e2e.test.js --runInBand --detectOpenHandles",
    "test:e2e:mqtt": "jest tests/mqtt-realtime-e2e.test.js --runInBand --detectOpenHandles",
    "test:e2e:db": "jest tests/database-consistency-e2e.test.js --runInBand --detectOpenHandles",
    "test:e2e:upload": "jest tests/file-upload-pipeline-e2e.test.js --runInBand --detectOpenHandles",
    "test:system": "node run-system-tests.js",
    "test:system:load": "node tests/load-test-runner.js",
    "test:system:performance": "node tests/performance-monitor.js",
    "test:system:memory": "node --expose-gc tests/memory-usage-monitor.js",
    "test:system:uat": "node tests/user-acceptance-test.js",
    "test:system:comprehensive": "node --expose-gc tests/run-comprehensive-system-tests.js",
    "integration:init": "node scripts/startSystemIntegration.js",
    "integration:test": "node scripts/runIntegrationTests.js",
    "integration:verify-mqtt": "node scripts/verifyMqttBroker.js",
    "integration:full": "npm run integration:init && npm run integration:test"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "proxy": "http://localhost:3010",
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "form-data": "^4.0.0",
    "jest": "^30.2.0",
    "nodemon": "^3.0.1",
    "supertest": "^7.1.4"
  }
}
